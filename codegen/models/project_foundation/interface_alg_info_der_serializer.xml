<interface name="alg info der serializer">
    Provide serialization of algorithms

    <method name="to der data">
        Serializer of algorithm information from public key in DER to buffer

        <argument name="alg info" interface="alg info"/>
        <argument name="der data" class="buffer" access="readwrite">
            <length argument="len"/>
        </argument>
        <return enum="error"/>
    </method>

</interface>
