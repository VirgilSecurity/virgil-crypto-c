<class name="pem title" context="none" scope="internal">
    Contains null-terminated string constants with known PEM titles.

    <variable name="public key" type="string">
        Contains constant string "PUBLIC KEY".

        <value value="PUBLIC KEY" type="string"/>
        <string access="readonly"/>
    </variable>


    <variable name="public key len" type="size">
        Contains length in bytes of string "PUBLIC KEY".

        <value value="sizeof(.(class_pem_title_variable_public_key)) - 1"/>
    </variable>


    <variable name="private key" type="string">
        Contains constant string "PRIVATE KEY".

        <value value="PRIVATE KEY" type="string"/>
        <string access="readonly"/>
    </variable>


    <variable name="private key len" type="size">
        Contains length in bytes of string "PRIVATE KEY".

        <value value="sizeof(.(class_pem_title_variable_private_key)) - 1"/>
    </variable>


    <variable name="encrypted private key" type="string">
        Contains constant string "ENCRYPTED PRIVATE KEY".

        <value value="ENCRYPTED PRIVATE KEY" type="string"/>
        <string access="readonly"/>
    </variable>


    <variable name="encrypted private key len" type="size">
        Contains length in bytes of string "ENCRYPTED PRIVATE KEY".

        <value value="sizeof(.(class_pem_title_variable_encrypted_private_key)) - 1"/>
    </variable>
</class>
