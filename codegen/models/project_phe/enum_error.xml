<enum name="error" definition="public">
    <constant name="success" of_class="global" value="0">
        No errors was occurred.
    </constant>

    <constant name="invalid success proof">
        Success proof check failed.
    </constant>

    <constant name="invalid fail proof">
        Failure proof check failed.
    </constant>

    <constant name="RNG error">
        RNG returned error
    </constant>

    <constant name="Protobuf decode error">
        Protobuf decode failed
    </constant>

    <constant name="Invalid ecp">
        Invalid ECP
    </constant>

    <constant name="Invalid private key">
        Invalid private key
    </constant>
</enum>
