<class name="key handler list">
    Handles a list of "key handler" class objects.

    <property name="item" class="key handler"/>
    <property name="next" class="self"/>
    <property name="prev" class="self"/>

    <method name="add" scope="private">
        Add new item to the list.
        Note, ownership is transferred.

        <argument name="key handler" class="key handler" access="disown"/>
    </method>


    <method name="remove self" visibility="private">
        Remove current node.
    </method>


    <method name="has item" is_const="1">
        Return true if given list has item.

        <return type="boolean"/>
    </method>


    <method name="item" is_const="1">
        Return list item.

        <return class="key handler" access="readonly"/>
    </method>


    <method name="has next" is_const="1">
        Return true if list has next item.

        <return type="boolean"/>
    </method>


    <method name="next" is_const="1">
        Return next list node if exists, or NULL otherwise.

        <return class="self" access="readonly"/>
    </method>


    <method name="has prev" is_const="1">
        Return true if list has previous item.

        <return type="boolean"/>
    </method>


    <method name="prev" is_const="1">
        Return previous list node if exists, or NULL otherwise.

        <return class="self" access="readonly"/>
    </method>


    <method name="clear">
        Remove all items.
    </method>


    <method name="find with identity" is_const="1">
        Find first key handler by it's identity.

        <argument name="identity" class="str"/>
        <argument name="error" class="error"/>

        <return class="key handler"/>
    </method>


    <method name="find with key id" is_const="1">
        Find key handler by it's key identifier.

        <argument name="key id" class="data"/>
        <argument name="error" class="error"/>

        <return class="key handler"/>
    </method>
</class>
