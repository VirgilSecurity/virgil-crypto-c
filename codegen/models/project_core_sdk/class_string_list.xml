<class name="string list">
    Handles a list of "string" class objects.

    <property name="item" class="str buffer" access="readonly"/>
    <property name="next" class="self"/>
    <property name="prev" class="self"/>


    <method name="add">
        Add new item to the list.

        <argument name="str" class="str"/>
    </method>


    <method name="add copy" scope="private">
        Add new item to the list.
        Note, string buffer is copied.

        <argument name="str buffer" class="str buffer" access="readonly"/>
    </method>


    <method name="add disown" scope="private">
        Add new item to the list.
        Note, ownership is transferred.

        <argument name="str buffer" class="str buffer" access="disown"/>
    </method>


    <method name="remove self" visibility="private">
        Remove current node.
    </method>


    <method name="has item" is_const="1">
        Return true if given list has item.

        <return type="boolean"/>
    </method>


    <method name="item" is_const="1">
        Return list item.

        <return class="str" access="readonly"/>
    </method>


    <method name="has next" is_const="1">
        Return true if list has next item.

        <return type="boolean"/>
    </method>


    <method name="next" is_const="1">
        Return next list node if exists, or NULL otherwise.

        <return class="self" access="readonly"/>
    </method>


    <method name="has prev" is_const="1">
        Return true if list has previous item.

        <return type="boolean"/>
    </method>


    <method name="prev" is_const="1">
        Return previous list node if exists, or NULL otherwise.

        <return class="self" access="readonly"/>
    </method>


    <method name="clear">
        Remove all items.
    </method>


    <method name="count" is_const="1">
        Return number of items within list.

        <return type="size"/>
    </method>


    <method name="contains" is_const="1">
        Return true if list contains a given value.

        <argument name="str" class="str"/>

        <return type="boolean"/>
    </method>
</class>
