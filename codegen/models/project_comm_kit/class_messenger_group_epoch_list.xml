<class name="messenger group epoch list">
    Handles a list of "messenger group epoch" class objects.

    <property name="item" class="messenger group epoch"/>
    <property name="next" class="self"/>
    <property name="prev" class="self"/>

    <method name="add" scope="private">
        Add new item to the list.
        Note, ownership is transfered.

        <argument name="messenger group epoch" class="messenger group epoch" access="disown"/>
    </method>


    <method name="remove self" visibility="private">
        Remove current node.
    </method>


    <method name="has item" is_const="1">
        Return true if given list has item.

        <return type="boolean"/>
    </method>


    <method name="item" is_const="1">
        Return list item.

        <return class="messenger group epoch" access="readonly"/>
    </method>


    <method name="has next" is_const="1">
        Return true if list has next item.

        <return type="boolean"/>
    </method>


    <method name="next" is_const="1">
        Return next list node if exists, or NULL otherwise.

        <return class="self" access="readonly"/>
    </method>


    <method name="has prev" is_const="1">
        Return true if list has previous item.

        <return type="boolean"/>
    </method>


    <method name="prev" is_const="1">
        Return previous list node if exists, or NULL otherwise.

        <return class="self" access="readonly"/>
    </method>


    <method name="clear">
        Remove all items.
    </method>


    <method name="latest epoch" is_const="1">
        Return latest epoch.

        <return class="messenger group epoch" access="readonly"/>
    </method>


    <method name="find" is_const="1">
        Find epoch by it's number.

        <argument name="epoch num" type="size"/>
        <argument name="error" class="error"/>

        <return class="messenger group epoch"/>
    </method>
</class>

